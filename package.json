{
  "name": "campaign-manager",
  "version": "1.0.0",
  "description": "Eine moderne Webanwendung f√ºr die effiziente Verwaltung von Wahlkampagnen",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "client-install": "cd client && npm install",
    "client-build": "cd client && CI=false npm run build",
    "build": "npm run client-install && npm run client-build",
    "postinstall": "npm run build"
  },
  "keywords": [
    "campaign",
    "management",
    "nodejs",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.21.1",
    "express-rate-limit": "^6.11.2",
    "express-session": "^1.18.1",
    "helmet": "^5.1.1",
    "joi": "^17.13.3",
    "mongoose": "^6.13.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "concurrently": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  }
}
